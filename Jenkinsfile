pipeline {
	agent any 
		stages{
			stage('1-make a left'){
				steps{
				sh  'echo "walk.."'
				}
			}
			stage('2-make a right'){
				steps{
					sh 'echo "walk..."'
				}
			}
			stage('3-make another left'){
				steps{
					sh 'echo "walk...."'
				}
			}
			stage('4-cross the street'){
				steps{
					sh 'echo "walk...."'
				}
			}
            stage('5-stop'){
				steps{
					sh 'echo "stopping...."'
		}
}
           stage('6-call mom'){
				steps{
					sh 'echo "callinging...."'
                }
           }

        }
}